{% extends 'admin/base.html.twig' %}

{% block title %}Création d'un projet {{ title_base }}{% endblock %}
{% block page_title %}Créer un projet{% endblock %}
{% block page_pretitle %}Projets{% endblock %}

{% form_theme form with [
    'admin/project/_collection.html.twig'
] %}

{% block page_content %}
<div class="container mb-5" id="project">
    {{ form_start(form) }}

    {{ form_widget(form) }}

    <button type="submit" class="btn btn-primary">Créer le projet</button>

    {{ form_end(form) }}
</div>
{% endblock %}
